<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
    <bean id="manager" class="edu.mcw.rgd.Manager">
        <property name="version" value="--Mouse Disease Annotation pipeline  -- build Dec 12, 2023" />
        <property name="sourcePipeline" value="MouseDO" />
        <property name="remoteDoFile" value="http://www.informatics.jax.org/downloads/reports/MGI_DO.rpt" />
        <property name="localDoFile" value="data/MGI_DO.rpt.gz" />
        <property name="createdBy" value="72" />
        <property name="staleAnnotDeleteThreshold" value="5%" />

        <property name="refRgdId" value="13592920" />
        <property name="evidenceCode" value="IAGP" />
        <property name="evidenceCode2" value="ISS" /> <!-- evidence code for orthologous annotations -->

        <property name="refRgdIdOmimPipeline" value="7240710" />
        <property name="evidenceCodeOmimPipeline" value="IAGP" />

        <property name="refRgdIdCtdPipeline" value="11554173" />
        <property name="evidenceCodeCtdPipeline" value="EXP" />
